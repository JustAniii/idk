        pins = calc(x, kl, pins, 144)
    elif y <= kl / 16 * 11:
        pins = calc(x, kl, pins, 160)
    elif y <= kl / 16 * 12:
        pins = calc(x, kl, pins, 176)
    elif y <= kl / 16 * 13:
        pins = calc(x, kl, pins, 192)
    elif y <= kl / 16 * 14:
        pins = calc(x, kl, pins, 208)
    elif y <= kl / 16 * 15:
        pins = calc(x, kl, pins, 224)
    else:
        pins = calc(x, kl, pins, 240)

